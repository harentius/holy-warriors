<!DOCTYPE html>
<html lang="en">
    <head>
        <title>Holy warriors</title>
        <meta charset="UTF-8">
        <meta name="description" content="" />
        <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1, user-scalable=no">

        <link href="css/style.css?{{ assetVersion }}" media="all" rel="stylesheet" type="text/css" />
    </head>

    <body>
        <div class="load">.</div>
        {% block before_body %}{% endblock %}
        {% block body %}{% endblock %}

        <script src="bower_components/phaser/build/phaser.min.js?{{ assetVersion }}"></script>
        <script src="bower_components/lodash/dist/lodash.min.js?{{ assetVersion }}"></script>
        {% block javascripts_vendor %}{% endblock %}
        <script>
            window.hw = window.hw || {};
            window.hw.assetVersion = '{{ assetVersion }}';
            window.hw.debug = {{ debug }};
        </script>
        <script src="js/bundle.js?{{ assetVersion }}"></script>
    </body>
</html>
